  "scripts": {
    "dev": "next dev",
    "dev:agentic": "node server/agentic-orchestrator-streaming.js",
    "dev:full": "dev-with-agentic-fixed.bat",
    "dev:ws": "cross-env ENABLE_WEBSOCKET=true WS_PORT=3001 node server/websocket-server.js",
    "dev:ws:win": "set ENABLE_WEBSOCKET=true && set WS_PORT=3001 && node server/websocket-server.js",
    "dev:collab": "node server/collaboration-server-simple.js",
    "dev:all": "node scripts/start-all.js",
    "dev:all:win": "start-all.bat",
    "dev:unified": "start-dev-unified.bat",
    "stop:unified": "stop-dev-unified.bat",
    "start:dev": "dev.bat",
    "stop:dev": "stop.bat",
    "build:ai-core": "tsc -p lib/ai-core/tsconfig.json",
    "build": "next build",
    "build:fast": "next build",
    "start": "NODE_ENV=production next start",
    "start:prod": "NODE_ENV=production ENABLE_AI_CORE=true next start",
    "test:security": "node tests/security/validate-k8s-integration.js",
    "test:executor": "node tests/security/test-secure-executor.js",
    "test:all-security": "npm run test:security && npm run test:executor",
    "verify-gke": "node -e \"const {getAICore} = require('./lib/ai-core-loader'); getAICore().then(ai => console.log(ai ? '✅ AI Core ready' : '❌ AI Core unavailable'))\"",
    "start:ws": "cross-env NODE_ENV=production ENABLE_WEBSOCKET=true WS_PORT=3001 node server/websocket-server.js",
    "start:ws:win": "set NODE_ENV=production && set ENABLE_WEBSOCKET=true && set WS_PORT=3001 && node server/websocket-server.js",
    "start:collab": "node server/collaboration-server-simple.js",
    "start:all": "cross-env NODE_ENV=production node scripts/start-all.js",
    "stop:all:win": "stop-all.bat",
    "lint": "next lint",
    "setup-db": "node scripts/setup-database.js",
    "migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "studio": "dotenv -e .env.local -- prisma studio",
    "db:pull": "dotenv -e .env.local -- prisma db pull",
    "ports": "node -e \"console.log(require('./config/ports').getSummary())\""
